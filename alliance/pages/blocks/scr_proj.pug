                          img.lazy( src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7" data-src="img/proj-img.jpg" alt="" width="125" height="90" data-object-fit-cover)

                        .proj__content
