mixin scr_contacts()
  section.scr_contacts.contacts(id="scr_contacts")
    .container
      .contacts__inner

      .contacts__df
        .contacts__content
          ul.contacts__nav
            li.active
              a(href="#contacts__nav-0") Москва
            li
              a(href="#contacts__nav-1") Симферополь

          ul.contacts__tabs
            li.contacts__tab#contacts__nav-0
              .contacts__links
                .contacts__link
                  a(href="tel:+79167034040" rel="nofollow") +7 916 703 40 40
                .contacts__link
                  a(href="mailto:dr.dalanna" rel="nofollow")
                    i.fa.fa-instagram
                    span dr.dalanna

              .contacts__address
