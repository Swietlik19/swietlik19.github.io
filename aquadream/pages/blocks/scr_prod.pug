mixin scr_prod()
  section.scr_prod.prod.grey-bg(id="scr_prod")
    .container
      .prod__top
        h2.page-title ТОП новинок 2020

        .prod__slider-btns
          button.swiper-button-prev
          .swiper-dots
          button.swiper-button-next

      .prod__slider#prod__slider
        .swiper-wrapper
          .swiper-slide.prod__slide
            .prod__item
              a.prod__img.fancybox(href="img-temp/prod/img-1.jpg")
                span.prod__label.prod__label--hit Хит

                noscript
                  img.lazy(src="img-temp/prod/img-1.jpg" alt="" width="125" height="90" data-object-fit-contain)
                img.lazy( src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7" data-src="img-temp/prod/img-1.jpg" alt="" width="125" height="90" data-object-fit-contain)

              .prod__content
                h2.prod__title Адаптер с внешней нарезкой
                  br
                  |  ERGO™ G1/2"

