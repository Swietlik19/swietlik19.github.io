mixin scr_prod()
  section.scr_prod.prod(id="scr_prod")
    .container
      .prod__cont
        ul.tabs-nav
          li.current
            a(href="#prod__tab-0") Акционный товар
          li
            a(href="#prod__tab-1") Новинки
          li
            a(href="#prod__tab-2") Товар с наибольшей выгодой

        ul.prod__tabs
          li.prod__tab#prod__tab-0
            .prod__wrap
              .prod__slider#prod__slider-0-0
                .swiper-wrapper
                  .swiper-slide.prod__slide
                    .prod__item
                      .prod__top
                        span.prod__label.prod__label--action Акция

                        .prod__btns
                          button.prod__btn(type="button")
                            svg.icon
                              use(xlink:href="img/svg/sprite.svg#icon-heart")

                      a.prod__img.fancybox(href="img-temp/prod/img-1.jpg")
                        noscript
                          img.lazy(src="img-temp/prod/img-1.jpg" alt="" width="125" height="90" data-object-fit-contain)
                        img.lazy( src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7" data-src="img-temp/prod/img-1.jpg" alt="" width="125" height="90" data-object-fit-contain)

                      .prod__main
                        .prod__content
                          h3.prod__title Набор воздушных шаров
                            br
                            |  «Сердца красные» 5 шт. 10"
                          .prod__desc
                            p «Flexmetal» Испания

                          span.prod__article Артикул:
                            strong  1202-2809

                        .prod__table
                          .prod__table-row
