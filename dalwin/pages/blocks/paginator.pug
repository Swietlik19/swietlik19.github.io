mixin paginator()
  .pager.clear
    ul.page-numbers
      li
        a.prev.page-numbers(href='#')
          svg.icon
            use(xlink:href="img/svg/sprite.svg#arrow")
          svg.icon
            use(xlink:href="img/svg/sprite.svg#arrow")
      li.current
        a.page-numbers(href='#') 01
      li
        span.page-numbers.current(aria-current="page") 02
      li
        a.page-numbers(href='#') 03
      li
        a.next.page-numbers(href='#')
          svg.icon
            use(xlink:href="img/svg/sprite.svg#arrow")
          svg.icon
            use(xlink:href="img/svg/sprite.svg#arrow")

  // /.paginator
