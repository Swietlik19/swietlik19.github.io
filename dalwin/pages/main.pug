    style.
      @keyframes sk-circle-bounce-delay {
        0%, 80%, 100% {
          transform: scale(0);
        }
        40% {
          transform: scale(1.0);
        }
      }
      .page_preloader {
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100vh;
        display: -webkit-flex;
        display: -moz-flex;
        display: -ms-flex;
        display: -o-flex;
        display: flex;
        -ms-align-items: center;
        align-items: center;
        justify-content: center;
        z-index: 1000;
        background-color: #f9f7f4;
      }
      .page_preloader::before {
        content: '';
        position: absolute;
        top: 30px;
        left: 30px;
        display: block;
        width: 70%;
        height: 70%;
        background: transparent url('img/svg/logo.svg') no-repeat center center;
        background-size: contain;
        opacity: 0.1;
        z-index: -1;
      }
      .page_preloader__circle {
        position: relative;
        width: 6em;
        height: 6em;
      }
      .page_preloader__circle .circle-child {
        width: 100%;
        height: 100%;
        position: absolute;
        left: 0;
        top: 0;
      }
      .page_preloader__circle .circle-child#circle-child-2 {
        -webkit-transform: rotate(30deg);
        transform: rotate(30deg);
      }
      .page_preloader__circle .circle-child#circle-child-2::before {
        -webkit-animation-delay: -1.1s;
        animation-delay: -1.1s;
      }
      .page_preloader__circle .circle-child#circle-child-3 {
        -webkit-transform: rotate(60deg);
        transform: rotate(60deg);
      }
      .page_preloader__circle .circle-child#circle-child-3::before {
        -webkit-animation-delay: -1s;
        animation-delay: -1s;
      }
      .page_preloader__circle .circle-child#circle-child-4 {
        -webkit-transform: rotate(90deg);
        transform: rotate(90deg);
      }
      .page_preloader__circle .circle-child#circle-child-4::before {
        -webkit-animation-delay: -0.9s;
        animation-delay: -0.9s;
      }
      .page_preloader__circle .circle-child#circle-child-5 {
        -webkit-transform: rotate(120deg);
        transform: rotate(120deg);
      }
      .page_preloader__circle .circle-child#circle-child-5::before {
        -webkit-animation-delay: -0.8s;
        animation-delay: -0.8s;
      }
      .page_preloader__circle .circle-child#circle-child-6 {
        -webkit-transform: rotate(150deg);
        transform: rotate(150deg);
      }
      .page_preloader__circle .circle-child#circle-child-6::before {
        -webkit-animation-delay: -0.7s;
        animation-delay: -0.7s;
      }
      .page_preloader__circle .circle-child#circle-child-7 {
        -webkit-transform: rotate(180deg);
        transform: rotate(180deg);
      }
      .page_preloader__circle .circle-child#circle-child-7::before {
        -webkit-animation-delay: -0.6s;
        animation-delay: -0.6s;
      }
      .page_preloader__circle .circle-child#circle-child-8 {
        -webkit-transform: rotate(210deg);
        transform: rotate(210deg);
      }
      .page_preloader__circle .circle-child#circle-child-8::before {
        -webkit-animation-delay: -0.5s;
        animation-delay: -0.5s;
      }
      .page_preloader__circle .circle-child#circle-child-9 {
        -webkit-transform: rotate(240deg);
        transform: rotate(240deg);
      }
      .page_preloader__circle .circle-child#circle-child-9::before {
        -webkit-animation-delay: -0.4s;
        animation-delay: -0.4s;
      }
      .page_preloader__circle .circle-child#circle-child-10 {
        -webkit-transform: rotate(270deg);
        transform: rotate(270deg);
      }
      .page_preloader__circle .circle-child#circle-child-10::before {
        -webkit-animation-delay: -0.3s;
        animation-delay: -0.3s;
      }
      .page_preloader__circle .circle-child#circle-child-11 {
        -webkit-transform: rotate(300deg);
        transform: rotate(300deg);
      }
      .page_preloader__circle .circle-child#circle-child-11::before {
        -webkit-animation-delay: -0.2s;
        animation-delay: -0.2s;
      }
      .page_preloader__circle .circle-child#circle-child-12 {
        -webkit-transform: rotate(330deg);
        transform: rotate(330deg);
      }
      .page_preloader__circle .circle-child#circle-child-12::before {
        -webkit-animation-delay: -0.1s;
        animation-delay: -0.1s;
      }
      .page_preloader__circle .circle-child::before {
        content: '';
        display: block;
        margin: 0 auto;
        width: 15%;
        height: 15%;
        background-color: #daa976;
        border-radius: 100%;
        -webkit-animation: sk-circle-bounce-delay 1.2s infinite ease-in-out both;
        animation: sk-circle-bounce-delay 1.2s infinite ease-in-out both;
      }
    +styles

