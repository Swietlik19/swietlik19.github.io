mixin scr_lead()
  section.scr_lead.lead(id="scr_lead")
    .container
      h2.page-title Получите визуализацию дизайна интерьеров под наши апартаменты

      .lead__df
        ul.lead__list
          li Более подробная информация о&nbsp;жилом комплексе
          li Изображения фасадов и&nbsp;внутреннего двора
          li Преимущества жилого комплекса

        .lead__form
          .form.wpcf7
            p.form__title Заполните короткую форму
            p.form__desc Чтобы получить визуализацию

            form.wpcf7-form
              .form__item
                span.wpcf7-form-control-wrap.your-name.text-input
                  input.wpcf7-form-control.wpcf7-text.wpcf7-name(type='text', name='your-name', value='', size='40', aria-required='true', aria-invalid='false', placeholder='Имя')

              .form__item
                span.wpcf7-form-control-wrap.your-tel.text-input
                  input.wpcf7-form-control.wpcf7-text.wpcf7-tel(type='tel', name='your-tel', value='', size='40', aria-required='true', aria-invalid='false', placeholder='+7 (___) ___-__-__')

              .form__item
                span.wpcf7-form-control-wrap.your-email.text-input
                  input.wpcf7-form-control.wpcf7-text.wpcf7-email(type='email', name='your-email', value='', size='40', aria-required='true', aria-invalid='false', placeholder='E-mail')

              .form__item.form__item--submit
                .btn__wrap.btn__wrap--blue
                  input.wpcf7-form-control.wpcf7-submit.xbtn.xbtn--blue(type='submit', value='получить визуализацию')

              .form__item
                span.wpcf7-form-control-wrap.accept
                  span.wpcf7-form-control.wpcf7-checkbox.wpcf7-validates-as-required
                    span.wpcf7-list-item
                      label
                        input(type='checkbox', name='accept', value='Согласие на обработку персональных данных', checked='checked')
                        span.wpcf7-list-item-label Согласие на обработку персональных данных
                          i.accept__icon

  // /.scr_lead

mixin scr_lead--2()
  section.scr_lead.lead.lead--2(id="scr_lead")
    .container
      .lead__df
        .lead__info
          h2.page-title Станьте владельцем коммерческой недвижимости премиум класса на&nbsp;берегу Чёрного моря

          ul.scr2__feat
            li
              .scr2__feat-item.wow.flipInY(data-wow-duration="0.8s" data-wow-delay="0.2s")
                span.scr2__feat-num.wow.fadeIn(data-wow-duration="0.8s" data-wow-delay="0.8s") 50
                  span  м
                    sup 2

                .scr2__feat-text
                  p Площадь от 50 м
                    sup 2
                    |  , с возможностью объединения нескольких помещений
            li
              .scr2__feat-item.wow.flipInY(data-wow-duration="0.8s" data-wow-delay="0.4s")
                span.scr2__feat-num.wow.fadeIn(data-wow-duration="0.8s" data-wow-delay="1s") 550 000
                  br
                  span  руб/м
                    sup 2

                .scr2__feat-text Стоимость от 550 000 руб/м
                  sup 2 со свободной планировкой и предчистовой отделкой

          .lead__btns
            .btn__wrap.btn__wrap--brown
              a.xbtn.xbtn--brown(href="#") Посмотреть планировку

        .lead__form
          .form.wpcf7
