doctype html
html(lang='ru')
  head
    meta(charset="UTF-8")

    title  |

    meta(name='description', content='')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1')
    meta(name='format-detection', content='telephone=no')

    // Template Basic Images Start
    //- link(rel='icon', sizes='32x32', href='img/favicon/favicon-32x32.png')
    //- link(rel='icon', sizes='16x16', href='img/favicon/favicon-16x16.png')
    // Template Basic Images End

    // Custom Browsers Color Start
    meta(name='theme-color', content='#01569b')
    // Custom Browsers Color End

    include blocks/scripts.pug
    include blocks/script-map.pug
    include blocks/styles.pug
    include blocks/header.pug

    include blocks/scr1.pug
    include blocks/scr_serv.pug
    include blocks/scr_profit.pug
    include blocks/scr_objects.pug
    include blocks/scr_lead.pug
    include blocks/scr_ideal.pug
    include blocks/scr_map.pug
    include blocks/scr_brands.pug

    include blocks/footer.pug
    include blocks/socials.pug

    +styles

  body

    +header("div")

    #page_wr.page_wr
      +scr1--landing
      +scr_serv
      +scr_profit
      +scr_objects
      +scr_lead--landing
      +scr_ideal
      +scr_map--landing
      +scr_brands--landing


      a.to-top(href="#page_wr")
        svg.icon
          use(xlink:href="img/svg/sprite.svg#arrow")

    +footer("div")

    +scripts
    +script-map
