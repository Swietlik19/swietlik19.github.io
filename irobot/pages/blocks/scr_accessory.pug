                    use(xlink:href="img/svg/sprite.svg#icon-arrow-orange")

          ul.accessory__tabs-list
            li.accessory__tab#accessory-tab-0
              .accessory__brand#brand-tab-0
                .accessory__slider.slider1#accessory__slider-0
                  .accessory__slides.slider1__slides
                    .accessory__slide.slider1__slide
                      .accessory__item
                        a.accessory__img.fancybox(href="img-temp/img-prod-3.png")
