mixin scr_accessory()
  section.scr_accessory.accessory(id="scr_accessory")
    .container
      .accessory__df
        aside.card__nav.card__nav--accessory
          ul.card__nav-list
            li.card__nav-item.active
              a(href="#accessory-tab-0") Координаторы и ограничители движения
            li.card__nav-item
              a(href="#accessory-tab-1") Пульты дистанционного управления
            li.card__nav-item
              a(href="#accessory-tab-2") Щетки и скребки
            li.card__nav-item
              a(href="#accessory-tab-3") Фильтры
            li.card__nav-item
              a(href="#accessory-tab-4") Чистящий модуль
            li.card__nav-item
              a(href="#accessory-tab-5") Аккумуляторные батареи

        .accessory__tabs
          .accessory__top
            .accessory__left
              h2.title Аксессуары
              ul.accessory__brands#accessory-tab-0--brands
                li.accessory__brands-item.active
                  a(href="#brand-tab-0") Braava
                li.accessory__brands-item
                  a(href="#brand-tab-1") Roomba

              ul.accessory__brands#accessory-tab-1--brands
                li.accessory__brands-item.active
                  a(href="#brand-tab-2") Braava
                li.accessory__brands-item
                  a(href="#brand-tab-3") Roomba
            .accessory__right
              a.accessory__link(href="#" download)
