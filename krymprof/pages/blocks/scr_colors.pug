mixin scr_colors()
  section.scr_colors.colors(id="scr_colors")
    .wr
      .colors__df
        .colors__left
          h2.title2 карта цветов
          p.sandwich-page__desc Возможен индивидуальный заказ по каталогу

          h3 Стандартные цвета по каталогу RAL

          .colors__img--left.colors__img
            img(src="img/img-colors-1.jpg", alt="цвета по каталогу рал")

        aside.colors__right
          h3 Цвета PRINTECH:

          .colors__img--right.colors__img
            img(src="img/img-colors-2.jpg", alt="цвета принтех")




  // /.scr_colors
