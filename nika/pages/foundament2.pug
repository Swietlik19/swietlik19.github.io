doctype html
html(lang='ru')
  head
    meta(charset="UTF-8")

    title  |

    meta(name='description', content='')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1')
    meta(name='format-detection', content='telephone=no')

    // Template Basic Images Start
    //- link(rel='icon', sizes='32x32', href='img/favicon/favicon-32x32.png')
    //- link(rel='icon', sizes='16x16', href='img/favicon/favicon-16x16.png')
    // Template Basic Images End

    // Custom Browsers Color Start
    meta(name='theme-color', content='#166d3a')
    // Custom Browsers Color End

    include blocks/scripts.pug
    include blocks/script-map.pug
    include blocks/styles.pug
    include blocks/header.pug

    include blocks/scr1.pug
    include blocks/scr_found-prod.pug
    include blocks/scr_lead.pug
    include blocks/scr_foundament.pug
    include blocks/scr_buildings.pug

    include blocks/footer.pug
    include blocks/socials.pug

    +styles

  body

    +header("a"," header--second")

    #page_wr.page_wr

      +scr1--foundament
      +scr_found-prod
      +scr_lead--foundament
      +scr_foundament--2
      +scr_buildings

      a.to-top(href="#page_wr")
        svg.icon
          use(xlink:href="img/svg/sprite.svg#arrow")

    +footer("a")

    +scripts
    //- +script-map
