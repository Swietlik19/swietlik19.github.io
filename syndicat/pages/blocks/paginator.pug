mixin paginator()
  ul.paginator
    li
      a.prev(href='#')

    li.current
      span.page-numbers 1
    li
      a.page-numbers(href='#') 2
    li
      a.page-numbers(href='#') 3
    li
      a.page-numbers(href='#') 4
    li
      a.page-numbers(href='#') 5
    li
      a.page-numbers(href='#') 6
    li
      a.page-numbers(href='#') 7
    li
      a.next(href='#')


  // /.paginator
